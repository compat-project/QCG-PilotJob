[
  {
	"request": "submit",
	"jobs": [ {
		"name": "__api_client_${rcnt}",
		"execution": {
		  "exec": "python",
		  "args": [ "{API_CLIENT_SCRIPT}" ],
		  "stdout": "__api_client.stdout",
		  "stderr": "__api_client.stderr"
		},
		"resources": {
		  "numCores": {
				"exact": 1
		  }
		}
	} ]
  },
  {
	"request": "control",
	"command": "finishAfterAllTasksDone"
  }
]
